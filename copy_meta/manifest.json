{
	"name": "Copy meta",
	"manifest_version": 2,
	"version": "0.1",
	"description": "Copy meta itemprop url content",
	"browser_action":
	{
		"default_logo": "stupid_logo.png",
		"default_title": "Copy meta"
	},
	"icons":
	{
		"48": "stupid_logo.png"
	},
	"background":
	{
		"scripts": ["background.js"]
	},
	"content_scripts":
	[
		{
			"matches": ["*://books.google.com/ngrams/*"],
			"js": ["content.js"]
		}
	],
	"permissions":
	[
		"activeTab"
	]
}