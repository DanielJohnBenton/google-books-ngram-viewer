{
	"name": "Copy meta",
	"manifest_version": 2,
	"version": "0.1",
	"description": "Generate file name from Google Ngram Viewer URL and copy it to the clipboard",
	"browser_action":
	{
		"default_logo": "stupid_logo.png",
		"default_title": "Generate/copy Ngram Viewer screenshot filename"
	},
	"icons":
	{
		"48": "stupid_logo.png"
	},
	"background":
	{
		"scripts": ["background.js"]
	},
	/*"content_scripts":
	[
		{
			"matches": ["*://books.google.com/ngrams/*"],
			"js": ["content.js"]
		}
	],*/
	"permissions":
	[
		"activeTab"
	]
}